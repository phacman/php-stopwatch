{
    "name": "phacman/php-stopwatch",
    "type": "library",
    "description": "Compact version: Provides a way to profile code",
    "keywords": ["php", "stopwatch", "compact", "version"],
    "homepage": "https://github.com/symfony/stopwatch",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Pavel Vasin",
            "email": "phacman@yandex.ru",
            "role": "Developer"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "format-code": "php-cs-fixer fix --allow-risky=yes"
    },
    "require": {
        "php": ">=8.1",
        "symfony/service-contracts": "^2.5|^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3"
    },
    "autoload": {
        "psr-4": {
            "PhacMan\\Stopwatch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhacMan\\Stopwatch\\Tests\\": "tests/"
        }
    }
}
